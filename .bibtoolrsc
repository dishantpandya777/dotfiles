# load additional resource files
resource braces

# pretty printing options in alphabetical order

key.format = short
preserve.key.case=on
preserve.keys=on
print.align.key=1
print.align.string=50
print.align=20
print.indent=2
print.line.length=999999
print.use.tab=off
print.wide.equal=on
sort=on
suppress.initial.newline=on
verbose=on

count.all=on
count.used=on

check.double=on

rewrite.rule { "^\" *\"$"}
rewrite.rule { "^{ *}$" }

rewrite.rule { booktitle # "IEEE" = "" }
rewrite.rule { booktitle # "ACM" = "" }
rewrite.rule { booktitle # "/" = "" }

rewrite.rule { journal  # "IEEE " = "" }
rewrite.rule { journal  # "ACM " = "" }
rewrite.rule { journal  # "/" = "" }

sort.order { * = author # title # booktitle # journal # volume # number # year }

rename.field {pages = optpages}
